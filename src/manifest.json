{
  "name": "Takeover",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons" : {
    "128": "images/glg.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["takeover.litcoffee", "xhr.litcoffee"]
  }],
  "web_accessible_resources": [
    "xhr.js",
    "takeover.js",
    "takeover-app.html",
    "polymer/platform.js",
    "polymer/platform.js.map",
    "polymer/polymer.html",
    "polymer/layout.html",
    "polymer/polymer.js",
    "polymer/polymer.js.map"
  ],
  "permissions": [
    "storage",
    "background",
    "identity",
    "pushMessaging",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ]
}
